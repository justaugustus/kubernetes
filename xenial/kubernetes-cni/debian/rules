#!/usr/bin/make -f
# -*- makefile -*-

#export DH_VERBOSE=1
CNI_RELEASE = 9d5e6e60e79491207834ae8439e80c943db65a69

build:
	echo noop

binary:
	curl -sSL --fail --retry 5 \
		"https://storage.googleapis.com/kubernetes-release/network-plugins/cni-$(CNI_RELEASE).tar.gz" \
		| tar xz
	dh_testroot
	dh_auto_install
	dh_shlibdeps
	dh_install
	dh_installdeb
	dh_gencontrol
	dh_md5sums
	dh_builddeb

%:
	dh $@
